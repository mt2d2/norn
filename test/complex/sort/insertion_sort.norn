def insertion_sort(x: IntAry): IntAry

	length: Int = 10

	for j: Int = 1, j < length, j = j + 1 in 
		key: Int = x[j]

		i: Int = j - 1

		while (i >= 0 && x[i] > key) in 
			x[i + 1] = x[i]
			i = i - 1
		end

		x[i+1] = key
	end
end

def main(): Void
	x: [10]Int
	j: Int = 10
	for i: Int = 0, i < 10, i = i + 1 in 
		x[i] = j
		j = j - 1
	end

	for i: Int = 0, i < 10, i = i + 1 in
		println(x[i])
	end

	insertion_sort(x)

	for i: Int = 0, i < 10, i = i + 1 in
		println(x[i])
	end
end